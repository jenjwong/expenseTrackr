{
  "name": "Gigster",
  "version": "1.0.0",
  "scripts": {
    "install-packages": "concurrently \"cd server && npm i\" \"cd client && npm i\"",
    "dev": "concurrently \"cd server && nodemon server.js\" \"cd client && npm run start\"",
    "test": "nodemon --exec  './node_modules/.bin/mocha --reporter spec --no-timeouts --recursive'",
    "headless-tests": "cd test-end-to-end && codeceptjs run --steps",
    "selenium-start": "cd test-end-to-end && selenium-standalone start",
    "test-update": "npm run test -- -u"
  },
  "browserslist": "last 2 versions",
  "dependencies": {
    "body-parser": "^1.17.1",
    "connect-flash": "^0.1.1",
    "connect-mongo": "^1.3.2",
    "cookie-parser": "^1.4.3",
    "dompurify": "0.8.5",
    "dotenv": "^4.0.0",
    "es6-promisify": "^5.0.0",
    "express": "^4.15.2",
    "express-session": "^1.15.3",
    "express-validator": "^3.1.2",
    "md5": "^2.2.1",
    "mocha": "^3.4.2",
    "moment": "2.17.1",
    "mongoose": "^4.8.7",
    "mongoose-mongodb-errors": "0.0.2",
    "multer": "1.3.0",
    "numeral": "^2.0.6",
    "passport": "^0.3.2",
    "passport-local": "1.0.0",
    "passport-local-mongoose": "^4.0.0",
    "react-faux-dom": "^4.0.3",
    "redux-thunk": "^2.2.0",
    "validator": "^7.0.0"
  },
  "devDependencies": {
    "autoprefixer": "6.7.7",
    "babel-core": "6.24.0",
    "babel-loader": "6.4.0",
    "babel-preset-es2015": "6.24.0",
    "codeceptjs": "^0.6.3",
    "concurrently": "^3.4.0",
    "css-loader": "0.27.3",
    "extract-text-webpack-plugin": "2.1.0",
    "node-sass": "4.5.0",
    "nodemon": "1.11.0",
    "postcss-loader": "1.3.3",
    "react-scripts": "1.0.7",
    "sass-loader": "6.0.3",
    "webdriverio": "^4.8.0",
    "webpack": "2.2.1"
  }
}
